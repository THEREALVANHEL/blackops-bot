# Discord Bot
DISCORD_BOT_TOKEN=your_discord_bot_token
SERVER_ID=123456789012345678

# MongoDB (optional)
MONGODB_URI=mongodb+srv://user:pass@cluster.example.mongodb.net/?retryWrites=true&w=majority
MONGODB_DATABASE=blackops-bot

# Google Gemini
GEMINI_API_KEY=your_gemini_api_key

# Roles (optional)
MODERATOR_ROLE_ID=0
LEAD_MODERATOR_ROLE_ID=0
OVERSEER_ROLE_ID=0

# Bot restart auth (optional)
RESTART_AUTH_KEY=change_me

# Web server
PORT=5000
